{
  "name": "ng-simple-state-management",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:toolkit": "ng build --project=ngssm-toolkit --configuration=production",
    "test:toolkit": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-toolkit --code-coverage",
    "build:store": "ng build --project=ngssm-store --configuration=production",
    "test:store": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-store --code-coverage",
    "build:schematics": "ng build --project=ngssm-schematics && cd projects/ngssm-schematics && npm run build",
    "link:schematics": "npm link ./dist/ngssm-schematics --configuration=production",
    "build:remote-data": "ng build --project=ngssm-remote-data --configuration=production",
    "build:navigation": "ng build --project=ngssm-navigation --configuration=production",
    "test:navigation": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-navigation --code-coverage",
    "build:ag-grid": "ng build --project=ngssm-ag-grid --configuration=production",
    "test:ag-grid": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-ag-grid --code-coverage",
    "build:shell": "ng build --project=ngssm-shell --configuration=production",
    "test:shell": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-shell --code-coverage",
    "build:ace-editor": "ng build --project=ngssm-ace-editor --configuration=production",
    "build:ci": "npm run build:toolkit && npm run build:store && npm run build:schematics && npm run build:remote-data && npm run build:navigation && npm run build:ag-grid && npm run build:shell && npm run build:ace-editor",
    "test:ci": "npm run test:toolkit && npm run test:store && npm run test:navigation && npm run test:ag-grid && npm run test:shell",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.6",
    "@angular/cdk": "14.2.5",
    "@angular/common": "14.2.6",
    "@angular/compiler": "14.2.6",
    "@angular/core": "14.2.6",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "14.2.6",
    "@angular/material": "14.2.5",
    "@angular/platform-browser": "14.2.6",
    "@angular/platform-browser-dynamic": "14.2.6",
    "@angular/router": "14.2.6",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "ace-builds": "1.11.2",
    "ag-grid-angular": "28.2.0",
    "ag-grid-community": "28.2.0",
    "ag-grid-enterprise": "28.2.0",
    "immutability-helper": "^3.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.6",
    "@cypress/schematic": "^2.2.0",
    "@types/jasmine": "4.3.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "14.2.1",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "4.8.4",
    "cypress": "latest"
  }
}
