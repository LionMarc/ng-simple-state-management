{
  "name": "ng-simple-state-management",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:toolkit": "ng build --project=ngssm-toolkit --configuration=production",
    "test:toolkit": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-toolkit --code-coverage",
    "build:store": "ng build --project=ngssm-store --configuration=production",
    "test:store": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-store --code-coverage",
    "build:schematics": "ng build --project=ngssm-schematics && cd projects/ngssm-schematics && npm run build",
    "link:schematics": "npm link ./dist/ngssm-schematics --configuration=production",
    "build:remote-data": "ng build --project=ngssm-remote-data --configuration=production",
    "test:remote-data": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-remote-data --code-coverage",
    "build:navigation": "ng build --project=ngssm-navigation --configuration=production",
    "test:navigation": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-navigation --code-coverage",
    "build:ag-grid": "ng build --project=ngssm-ag-grid --configuration=production",
    "test:ag-grid": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-ag-grid --code-coverage",
    "build:shell": "ng build --project=ngssm-shell --configuration=production",
    "test:shell": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-shell --code-coverage",
    "build:tree": "ng build --project=ngssm-tree --configuration=production",
    "test:tree": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-tree --code-coverage",
    "build:ace-editor": "ng build --project=ngssm-ace-editor --configuration=production",
    "build:data": "ng build --project=ngssm-data --configuration=production",
    "test:data": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-data --code-coverage",
    "build:ci": "npm run build:store && npm run build:toolkit && npm run build:schematics && npm run build:remote-data && npm run build:navigation && npm run build:ag-grid && npm run build:ace-editor && npm run build:shell && npm run build:tree && npm run build:data",
    "test:ci": "npm run test:store && npm run test:toolkit && npm run test:remote-data && npm run test:navigation && npm run test:ag-grid && npm run test:shell && npm run test:tree && npm run test:data"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "ace-builds": "1.32.7",
    "ag-grid-angular": "31.1.1",
    "ag-grid-community": "31.1.1",
    "ag-grid-enterprise": "31.1.1",
    "immutability-helper": "^3.1.1",
    "luxon": "^3.4.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "5.1.4",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jasmine-core": "5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.4.2"
  }
}