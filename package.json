{
  "name": "ng-simple-state-management",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:toolkit": "ng build --project=ngssm-toolkit --configuration=production",
    "test:toolkit": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-toolkit --code-coverage",
    "build:store": "ng build --project=ngssm-store --configuration=production",
    "test:store": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-store --code-coverage",
    "build:schematics": "ng build --project=ngssm-schematics && cd projects/ngssm-schematics && npm run build",
    "link:schematics": "npm link ./dist/ngssm-schematics --configuration=production",
    "build:remote-data": "ng build --project=ngssm-remote-data --configuration=production",
    "test:remote-data": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-remote-data --code-coverage",
    "build:navigation": "ng build --project=ngssm-navigation --configuration=production",
    "test:navigation": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-navigation --code-coverage",
    "build:ag-grid": "ng build --project=ngssm-ag-grid --configuration=production",
    "test:ag-grid": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-ag-grid --code-coverage",
    "build:shell": "ng build --project=ngssm-shell --configuration=production",
    "test:shell": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-shell --code-coverage",
    "build:tree": "ng build --project=ngssm-tree --configuration=production",
    "test:tree": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-tree --code-coverage",
    "build:ace-editor": "ng build --project=ngssm-ace-editor --configuration=production",
    "build:ci": "npm run build:store && npm run build:toolkit && npm run build:schematics && npm run build:remote-data && npm run build:navigation && npm run build:ag-grid && npm run build:ace-editor && npm run build:shell && npm run build:tree",
    "test:ci": "npm run test:store && npm run test:toolkit && npm run test:remote-data && npm run test:navigation && npm run test:ag-grid && npm run test:shell && npm run test:tree"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.7",
    "@angular/cdk": "16.1.6",
    "@angular/common": "16.1.7",
    "@angular/compiler": "16.1.7",
    "@angular/core": "16.1.7",
    "@angular/forms": "16.1.7",
    "@angular/material": "16.1.6",
    "@angular/platform-browser": "16.1.7",
    "@angular/platform-browser-dynamic": "16.1.7",
    "@angular/router": "16.1.7",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "ace-builds": "1.23.4",
    "ag-grid-angular": "30.0.5",
    "ag-grid-community": "30.0.5",
    "ag-grid-enterprise": "30.0.5",
    "immutability-helper": "^3.1.1",
    "luxon": "^3.3.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.6",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "16.1.6",
    "@angular/compiler-cli": "16.1.7",
    "@types/jasmine": "4.3.5",
    "@types/luxon": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jasmine-core": "4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "ng-mocks": "14.11.0",
    "ng-packagr": "16.1.0",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.1.6"
  }
}